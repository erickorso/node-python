{
  "name": "node-python",
  "version": "1.0.0",
  "description": "A simple TypeScript API deployed on Vercel to test Python functions",
  "scripts": {
    "dev": "ts-node-dev api/index.ts",
    "sass:build": "sass public/styles.scss public/styles.css --style=compressed",
    "sass:watch": "sass public/styles.scss public/styles.css --watch",
    "build": "tsc && npm run sass:build",
    "start": "node build/index.js",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "sass": "^1.91.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "nodejs",
    "vercel",
    "api",
    "typescript",
    "python",
    "serverless"
  ],
  "author": "Erick Vargas",
  "license": "MIT"
}
